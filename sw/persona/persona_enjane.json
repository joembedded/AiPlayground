{
  "setup": {
    "turns": 10
  },
  "systemprompt": "${systemprompt_enjane.txt}",
  "payload": {
    "model": "gpt-4.1-mini",
    "input": "***MESSAGES-Array-of-Objects***",
    "temperature": 1,
    "max_output_tokens": 600,
    "store": false,
    "text": {
      "format": {
        "type": "json_schema",
        "name": "answer_schema_with",
        "strict": true,
        "schema": {
          "type": "object",
          "properties": {
            "answer": {
              "type": "object",
              "properties": {
                "text": { "type": "string" }
              },
              "required": ["text"],
              "additionalProperties": false
            }
          },
          "required": ["answer"],
          "additionalProperties": false
        }
      }
    }
  }
}
