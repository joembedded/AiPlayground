{
  "setup": {
    "turns": 10
  },
  "": "",
  "systemprompt": "${systemprompt_envilo.txt}",
  "payload": {
    "model": "gpt-4.1-mini",
    "input": "***MESSAGES-Array-of-Objects***",
    "temperature": 0.6,
    "max_output_tokens": 600,
    "store": false,
    "text": {
      "format": {
        "type": "json_schema",
        "name": "answer_schema_with_meta",
        "strict": true,
        "schema": {
          "type": "object",
          "properties": {
            "answer": {
              "type": "object",
              "properties": {
                "text": {
                  "type": "string"
                }
              },
              "required": [
                "text"
              ],
              "additionalProperties": false
            },
            "meta": {
              "type": "object",
              "properties": {
                "topic": {
                  "type": "string",
                  "enum": [
                    "elf",
                    "technics",
                    "natur",
                    "stories",
                    "food",
                    "inappropriate"
                  ]
                },
                "topic_scores": {
                  "type": "object",
                  "properties": {
                    "elf": {
                      "type": "number"
                    },
                    "technics": {
                      "type": "number"
                    },
                    "natur": {
                      "type": "number"
                    },
                    "stories": {
                      "type": "number"
                    },
                    "food": {
                      "type": "number"
                    },
                    "inappropriate": {
                      "type": "number"
                    }
                  },
                  "required": [
                    "elf",
                    "technics",
                    "natur",
                    "stories",
                    "food",
                    "inappropriate"
                  ],
                  "additionalProperties": false
                },
                "reason": {
                  "type": "string"
                }
              },
              "required": [
                "topic",
                "topic_scores",
                "reason"
              ],
              "additionalProperties": false
            }
          },
          "required": [
            "answer",
            "meta"
          ],
          "additionalProperties": false
        }
      }
    }
  }
}